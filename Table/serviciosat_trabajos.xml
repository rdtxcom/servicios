<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : serviciosat_trabajos.xml
    Description:
        Estructura de la tabla serviciosat_trabajos.
-->
<table>
    <column>
        <name>cantidad</name>
        <type>double precision</type>
    </column>
    <column>
        <name>codagente</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>descripcion</name>
        <type>text</type>
    </column>
    <column>
        <name>estado</name>
        <type>integer</type>
    </column>
    <column>
        <name>fechafin</name>
        <type>date</type>
    </column>
    <column>
        <name>fechainicio</name>
        <type>date</type>
    </column>
    <column>
        <name>horafin</name>
        <type>time</type>
    </column>
    <column>
        <name>horainicio</name>
        <type>time</type>
    </column>
    <column>
        <name>idservicio</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>idtrabajo</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>observaciones</name>
        <type>text</type>
    </column>
    <column>
        <name>precio</name>
        <type>double precision</type>
    </column>
    <column>
        <name>referencia</name>
        <type>character varying(30)</type>
    </column>
    <constraint>
        <name>serviciosat_trabajos_pkey</name>
        <type>PRIMARY KEY (idtrabajo)</type>
    </constraint>
    <constraint>
        <name>ca_serviciosat_servicios</name>
        <type>FOREIGN KEY (idservicio) REFERENCES serviciosat (idservicio) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_serviciosat_variantes</name>
        <type>FOREIGN KEY (referencia) REFERENCES variantes (referencia) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
</table>